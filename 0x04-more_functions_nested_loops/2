#include "main.h"

/**
 * more_numbers - Prints numbers 0 to 14
 * ten times followed by new line
 * Returns nothing
 */

void more_numbers(void)
{
	int a;
	char b;
	char c;
	char d;

	for (a = '0'; a <= '9'; a++)
	{
		for (c = '0'; c <= '9'; c++)
		{
			_putchar(c);
		}
		for (d = '1'; d <= '4'; d++)
		{
			b = 10 + (int)(d);
			_putchar(b);	
		}
		_putchar('\n');
	}
}
